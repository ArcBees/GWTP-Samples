<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">

<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder' xmlns:g='urn:import:com.google.gwt.user.client.ui'>

    <ui:with field="tabsResources" type="com.gwtplatform.samples.tab.client.resources.TabsResources"/>

    <g:HTMLPanel addStyleNames="{tabsResources.style.tab} {tabsResources.style.rounded} {tabsResources.style.inactive}">
        <g:Hyperlink ui:field="hyperlink"/>
    </g:HTMLPanel>
</ui:UiBinder>
