<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">

<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder' xmlns:g='urn:import:com.google.gwt.user.client.ui'>

    <ui:with field="tabsResources" type="com.gwtplatform.samples.tab.client.resources.TabsResources"/>

    <g:FlowPanel>
        <g:HTMLPanel>
            <g:FlowPanel ui:field="tabPanel"/>
            <div style="clear: both;"></div>
            <div class="{tabsResources.style.horizontalBar}"/>
        </g:HTMLPanel>
        <g:FlowPanel addStyleNames="{tabsResources.style.container}" ui:field="tabContentContainer"/>
    </g:FlowPanel>
</ui:UiBinder>
